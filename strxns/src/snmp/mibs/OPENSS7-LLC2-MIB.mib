-- =====================================================================
-- 
-- Notifications

lLCConnection2Event NOTIFICATION-TYPE
	OBJECTS	{
			tmnObjectInstance
			-- pdusDiscarded1
			-- pdusDiscarded2
			-- pduRetransmissions
			-- acknowledgeTimeout
			-- busyStateTimeout
			-- rejectTimeout
			-- pBitTimeout
			-- type2Violation
			-- retransmissions
		}
	STATUS		current
	DESCRIPTION
		"This event will be sent for the following condtions: 
		 DupIPDUs Received, PDUs Discarded1, PDUs Discarded2, 
		 PDU Retransmissions, Type2 Violation, Type2 
		 Acknowledgement Timeout Value, Type2 Reject Timeout 
		 Value, Type2 Busy-State, Timeout Value Type2, P-Bit 
		 Timeout Value, Retranmissions.  Fr each condition 
		 there is an appropriate parameter to be included in the 
		 notification."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-3), lLCConnection2Event-N."
	::= { llcNotifications 1 }


lLCClessACKEvent NOTIFICATION-TYPE
	OBJECTS	{
			tmnObjectInstance
			-- type3Violation
			-- retransmissions
			-- noResponse
		}
	STATUS		current
	DESCRIPTION
		"This event will be sent for the following conditions: 
		 Retransmission, NoResponse, Type3 Violation.  For each 
		 condition, there is an appropriate parameter to be 
		 included in the notification."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-3), lLCClessACKEvent-N."
	::= { llcNotifications 2 }

lLCStationEvent NOTIFICATION-TYPE
	OBJECTS	{
			tmnObjectInstance
			-- pdusDiscarded
			-- bufferProblems
			-- ?? retransmissions
		}
	STATUS		current
	DESCRIPTION
		"This notification will be sent ofr either 
		 pdusdiscarded, buffer problems, or Type 3 
		 retransmissions."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-3), lLCStationEvent-N."
	::= { llcNotifications 3 }


