typedef struct @{
        ulong dl_primitive;
@} dl_detach_req_t;
@tpindex dl_detach_req_t
@end example

@subsubheading Parameters

@vtable @var
@item dl_primitive
conveys
DL_@-DETACH_@-REQ.
@prindex DL_DETACH_REQ
@end vtable

