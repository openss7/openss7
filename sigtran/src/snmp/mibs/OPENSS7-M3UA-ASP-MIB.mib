	::= { m3uaAspSg 1 }

m3uaAspSgEntry OBJECT-TYPE
	SYNTAX		M3uaAspSgEntry
	MAX-ACCESS	not-accessible
	DESCRIPTION
		"Defines the characteristis of individual SG within an 
		 SG grouping."
	INDEX { m3uaAspAgIndex, m3uaAspSgIndex }
	::= { m3uaAspSgTable 1 }

M3uaAspSgEntry ::=
	SEQUENCE {
		m3uaAspSgIndex
			Unsigned32,
		m3uaAspSgName
			CommonLanaguageName,
		m3uaAspSgAdministrativeState
			AdministrativeState,
		m3uaAspSgOperationalState
			OperationalState,
		m3uaAspSgProcedurealStatus
			ProceduralStatus,
		m3uaAspSgUsgageState
			UsageState,
		m3uaAspSgAspState
			AspState, -- OPENSS7-SIGTRAN-TC
		m3uaAspSgMaxInitRetrans
			Unsigned32,
		m3uaAspSgMaxPathRetrans
			Unsigned32,
		m3uaAspSgMaxLifeTime
