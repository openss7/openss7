		m3uaAspRsRemotePointCode
			NetworkPointCode,
		m3uaAspRsOperationalState
			OperationalState,
		m3uaAspRsAdministrativeState
			AdministrativeState,
		m3uaAspRsUsageState
			UsageState,
		m3uaAspRsAlarmStatus
			AlarmStatus,
		m3uaAspRsProceduralStatus
			ProceduralStatus,
		m3uaAspRsTimerT8
			TimeInterval,
		m3uaAspRsTimerT11
			TimeInterval,
		m3uaAspRsTimerT15
			TimeInterval,
		m3uaAspRsTimerT16
			TimeInterval,
		m3uaAspRsTimerT18A
			TimeInterval,
		m3uaAspRsOptions
			MtpOptions,
		m3uaAspRsAvailabilityStatus
			MtpAvailabilityStatus,
		m3uaAspRsCongestionLevel
			MtpCongestionLevel,
		m3uaAspRsStatus
			RowStatus
	}

m3uaAspRsIndex OBJECT-TYPE
	SYNTAX		Unsigned32
	MAX-ACCESS	not-accessible
	STATUS		current
