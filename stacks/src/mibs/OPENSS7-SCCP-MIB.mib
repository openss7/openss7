-- used with the sccpOperationalProtocols attribute of the 
-- sccpLinkageTable entry.
--
sccpProtocolClass0 OBJECT IDENTIFIER ::= { sccpSpecificExtensions 24 }-- basic connectionless
sccpProtocolClass1 OBJECT IDENTIFIER ::= { sccpSpecificExtensions 25 }-- sequenced connectionless
sccpProtocolClass2 OBJECT IDENTIFIER ::= { sccpSpecificExtensions 26 }-- basic connection-oriented
sccpProtocolClass3 OBJECT IDENTIFIER ::= { sccpSpecificExtensions 27 }-- flow control connection oriented


-- =====================================================================

sccpNetworkEntityTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SccpNetworkEntityEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	"ITU-T Rec. Q.751.2 sccp MO."
    ::= { sccpNetworkEntity 1 }

sccpNetworkEntityEntry OBJECT-TYPE
    SYNTAX SccpNetworkEntityEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"The sccpNetworkEntityEntry is the placeholder for all SCCP 
	 related maanged objects.  No specific management aspects have 
	 ben identified.
	 
	 The communicationsEntityId is use for naming.
	 
	 The localSapNames attribute contains a set of distinguished 
	 names of SCCP SAPs at which services are provided to the 
	 entity.
	 
	 The systemTypes attributs is a set-valued attribute with 
	 possible values: End System (ES) indicates an SCCP destination 
	 node, Intermediate System (IS) indicates an SCCP relay node.
	 
	 The networkEntityTitles attribute unambiguously identifies the 
	 name of the SCCP in an end or intermediate node.  The value may 
	 be entered by a system management operation or it may be 
	 derived by some local means, for example by auto 
	 configuration."
    REFERENCE
	"ITU-T Rec. Q.751.2 sccp MO."
    INDEX   {
	    sccpNetworkEntityId
	}
    ::= { sccpNetworkEntityTable 1 }
	

SccpNetworkEntityEntry ::=
    SEQUENCE {
	sccpNetworkEntityId
	    SimpleNameType, -- index
	sccpNetworkEntityAlarmStatus
	    AlarmStatus,
	sccpCommunicationsEntityId
	    SimpleNameType,
	sccpLocalSapNames
	    OCTET STRING, -- needs to be a sidetable
	sccpNetworkEntityTitles
	    SccpNAddresses,
	sccpSystemTypes
	    NEntitySystemTypes,
	sccpVersion
	    DisplayString,
	sccpLUDTandLUDTSSupported
	    TruthValue,
	sccpCoordChangeTimer
	    TimeInterval,
	sccpIgnoreSSTTimer
	    TimeInterval,
	sccpMaxStatInfoTimer
	    TimeInterval,
	sccpNetworkEntityName
	    CommonLanguageName,
	sccpRowStatus
	    RowStatus
    }

sccpNetworkEntityId OBJECT-TYPE
    SYNTAX SimpleNameType
    MAX-ACCESS not-accessible
    STATUS current
    ::= { sccpNetworkEntityEntry 1 }

sccpNetworkEntityAlarmStatus OBJECT-TYPE
    SYNTAX AlarmStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"Provides the alarm status, following the AlarmStatus textual 
	 convention of the OPENSS7-SMI-MIB module, of the SCCP network 
	 entity corresponding to the conceptual row in which the status 
	 occurs.  This managed object generates the following alarms:

	 The communicationsAlarm can have the following probable causes:

	 - ProbableCause = localSccpUnAvailable which represents 
	   measurements Q.752/8.1 measurement Q.752/8.3 
	   (SpecificProblems = Failure, Maintenance, Congestion) 
	   (PerceivedSeverity = Major/Minor/Warning) and measurement 
	   Q.752/8.4 (PerceivedSeverity = Cleared),

	 - ProbableCause = subsystemOoSgranted which represents 
	   emasurement Q.752/8.6, and

	 - ProbableCause = subsystemOoSdenied which represents 
	   measurement Q.752/8.7."
    REFERENCE
	"ITU-T Rec. X.731 (1993) : 8.1.2.1, ITU-T Rec. X.721 (1882) : 
	 alarmStatus"
    ::= { sccpNetworkEntityEntry 2 }


sccpCommunicationsEntityId OBJECT-TYPE
    SYNTAX SimpleNameType
    MAX-ACCESS not-accessible
    STATUS current
    REFERENCE
	"ITU-T Rec. X.723 communicationsEntityId."
    ::= { sccpNetworkEntityEntry 3 }

-- need to implement as side-table
sccpLocalSapNames OBJECT-TYPE
    SYNTAX OCTET STRING
    MAX-ACCESS not-accessible
    STATUS current
    REFERENCE
	"ITU-T Rec. X.723 localSapNames."
    ::= { sccpNetworkEntityEntry 4 }

sccpNetworkEntityTitles OBJECT-TYPE
    SYNTAX SccpNAddresses
    MAX-ACCESS not-accessible
    STATUS current
    REFERENCE
	"ITU-T Rec. X.283 networkEntityTitles."
    ::= { sccpNetworkEntityEntry 5 }

sccpSystemTypes OBJECT-TYPE
    SYNTAX NEntitySystemTypes
    MAX-ACCESS read-create
    STATUS current
    REFERENCE
	"ITU-T Rec. X.283 systemTypes."
    ::= { sccpNetworkEntityEntry 6 }

sccpVersion OBJECT-TYPE
    SYNTAX DisplayString
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"This attribute is single-valued.  It refers to the 
	 recommendation on which the SCCP implementation is based.  It 
	 should really be an object identifier."
    REFERENCE
	"ITU-T Rec. Q.751.2 sccpVersion."
    ::= { sccpNetworkEntityEntry 7 }


sccpLUDTandLUDTSSupported OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"This attribute indicates whether the implementation supports 
	 LUTD(S) messages."
    REFERENCE
	"ITU-T Rec. Q.751.2 lUDTandLUDTSSupported."
    ::= { sccpNetworkEntityEntry 8 }

sccpCoordChangeTimer OBJECT-TYPE
    SYNTAX TimeInterval
    UNITS       "seconds"
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"This attribute represents the initial value of timer tCoordChg: 
	 waiting for grant for subsystem to go out of service, as 
	 defined in 5.3.5.2/Q.714."
    REFERENCE
	"ITU-T Rec. Q.751.2 coordChangeTimer."
    ::= { sccpNetworkEntityEntry 9 }

sccpIgnoreSSTTimer OBJECT-TYPE
    SYNTAX TimeInterval
    UNITS       "seconds"
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"This attribute represents the initial value of timer 
	 TignoreSST: delay for subsystem between receiving grant to go 
	 out of service and actually going out of service, as defined in 
	 3.5.2/Q.714."
    REFERENCE
	"ITU-T Rec. Q.751.2 ignoreSSTTimer."
    ::= { sccpNetworkEntityEntry 10 }

sccpMaxStatInfoTimer OBJECT-TYPE
    SYNTAX TimeInterval
    UNITS       "seconds"
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"This attribute represents the maximum value of timer TstatInfo: 
	 delay between requesets for subsystem status information, as 
	 defined in 5.3.4.2/Q.714."
    REFERENCE
	"ITU-T Rec. Q.751.2 maxStatInfoTimer."
    ::= { sccpNetworkEntityEntry 11 }

sccpNetworkEntityName OBJECT-TYPE
    SYNTAX CommonLanguageName
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"Provide an optional network entity name following COMMON 
	 LANGUAGE procedures and the CommonLanguageName textual 
	 convention."
    REFERENCE
	"ITU-T Rec. Q.751.2 (1992) networkEntityName."
    ::= { sccpNetworkEntityEntry 12 }

sccpRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	"Provides a means, according to the RowStatus textual 
	 convention, for management stations to create and delete rows 
	 from this table.  When created by management stations, entries 
	 in this table have no other entries in other tables associated 
	 with the newly created SCCP network entity.  Before an entry 
	 may be deleted by a management station, all side-tables that 
	 use the sccpNetworkEntityId as an index or which reference this 
	 row in the table using the RowPointer or SimpleNameType textual 
	 convention must be deleted or altered respectively."
    REFERENCE
	"STD 58, SNMPv2-TC RowStatus"
    ::= { sccpNetworkEntityEntry 13 }

sccpSapNameTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SccpSapNameEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	"This is a side-table to the sccpNetworkEntity table which 
	 provides a list of localSapNames that are associated with each 
	 entity."
    REFERENCE
	""
    ::= { sccpSapName 1 }

sccpSapNameEntry OBJECT-TYPE
    SYNTAX SccpSapNameEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	""
    INDEX   {
	    sccpNetworkEntityId, -- shared index
	    sccpSapId -- shared index
	}
    ::= { sccpSapNameTable 1 }

SccpSapNameEntry ::=
    SEQUENCE {
	sccpSapNameRowStatus
	    RowStatus
    }

sccpSapNameRowStatus OBJECT-TYPE
    SYNTAX RowStatus
    MAX-ACCESS read-create
    STATUS current
    DESCRIPTION
	""
    REFERENCE
	""
    ::= { sccpSapNameEntry 1 }

