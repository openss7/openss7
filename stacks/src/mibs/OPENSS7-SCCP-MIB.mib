	    incompatibleAddressLength(8),
	    expectedSSNotFound(9)
	}

PrimaryOrBackup ::= TEXTUAL-CONVENTION
    STATUS current
    DESCRIPTION
	"Provides a means for specifying whether an entity in an entity 
	 set is an equal(0), primary(1) or backup(2) member of the 
	 entity set.  When all memebrs of the entity set are equal(0), 
	 loadsharing will occur accross members of the entity set.  When 
	 one member is primary(1) and the other members are backup(2), 
	 the traffic load will be directed at the primary(1)."
    REFERENCE   
	"ITU-T Rec. Q.751.2 ASN.1 PrimaryOrBackup."
    SYNTAX INTEGER { equal(0), primary(1), backup(2) }

SharingMode ::= TEXTUAL-CONVENTION
