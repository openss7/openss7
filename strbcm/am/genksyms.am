endif
## !am_fastdepCC
endif

## KERNEL_VERSIONS
else
## ! KERNEL_VERSIONS

stamp-verfiles: $(EXPORT_SRCFILES)
	@list="$(EXPORT_SRCFILES)"; for s in $$list ; do \
		if test -f "$$s" ; then d=; else d="$(srcdir)/"; fi; \
		r=`$(ECHO) "$$s" | sed -e 's|^.*/||;s|\.c$$||'` ; \
		$(ECHO) "touch \"$$r.ver\"" ; \
		touch "$$r.ver" ; \
	done ; \
