clean-verfiles:
	@vers=`$(ECHO) " $(EXPORT_SRCFILES) " | sed -e 's| *  | |g;s| [^[:space:]]*/| |g;s|\.c |.ver |g'` ; \
	$(ECHO) "rm -f -- stamp-verfiles $$vers" ; \
	rm -f -- stamp-verfiles $$vers

CLEAN_LOCAL		+= clean-verfiles

stamp-verobjs: stamp-verfiles $(KERNEL_OBJECTS)
	@list="$?" ; for obj in $$list ; do \
		test :$$obj != :stamp-verfiles || continue ; \
		if $(OBJDUMP) -h $$obj | grep -q __ksymtab; then \
			a=`$(ECHO) "$$obj" | sed -e 's|^lib.*_a-|$(kpre)|'` ; \
			v=`$(ECHO) "$$obj" | sed -e 's|^lib.*_a-||;s|\.o$$||'` ; \
			$(ECHO) "$(LD) -r -o a.$$$$.out $(PKG_LDS) $$obj -T $$v.ver && mv -f -- a.$$$$.out $$a" ; \
			$(LD) -r -o a.$$$$.out $(PKG_LDS) $$obj -T $$v.ver && mv -f -- a.$$$$.out $$a ; \
		else \
			a=`$(ECHO) "$$obj" | sed -e 's|^lib.*_a-|$(kpre)|'` ; \
			$(ECHO) "$(LD) -r -o a.$$$$.out $(PKG_LDS) $$obj && mv -f -- a.$$$$.out $$a" ; \
			$(LD) -r -o a.$$$$.out $(PKG_LDS) $$obj && mv -f -- a.$$$$.out $$a ; \
		fi ; \
	done ; \
	touch stamp-verobjs

ALL_LOCAL		+= stamp-verobjs

clean-verobjs:
	@objs=`$(ECHO) " $(KERNEL_OBJECTS) " | sed -e 's| *  | |g;s| lib[^[:space:]]*_a-| $(kpre)|g'` ; \
	$(ECHO) "rm -f -- stamp-verobjs $$objs" ; \
	rm -f -- stamp-verobjs $$objs

