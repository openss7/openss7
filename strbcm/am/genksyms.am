$(MODVERSIONS_H): $(EXPORT_SRCFILES) $(EXPORT_VERFILES)
	@$(ECHO) "Creating $@ header file..." ; \
	v=`$(ECHO) "$(MODVERSIONS_H)" | sed -e s'|/[^/]*$$||'`/versions ; \
	$(mkdir_p) $$v ; \
	( \
		$(ECHO) "#ifndef _SYS_MODVERSIONS_H" ; \
		$(ECHO) "#define _SYS_MODVERSIONS_H" ; \
		vers='' ; \
		list='$(EXPORT_SRCFILES)'; \
		for s in $$list ; do \
			if test -f "$$s" ; then d=; else d="$(srcdir)/"; fi; \
			r=`$(ECHO) "$$s" | sed -e 's|^.*/||;s|\.c$$||'` ; \
			if test -f $$v/$$r.ver ; then \
				p=`$(ECHO) "$$v" | sed -e 's|^[^/]*/||'` ; \
				$(ECHO) "#include \"$$p/$$r.ver\"" ; \
				vers="$${vers}$${vers:+ }$$v/$$r.ver" ; \
			fi ; \
		done ; \
		$(ECHO) "#endif /* _SYS_MODVERSIONS_H */" ; \
	) >$(MODVERSIONS_H).tmp || { rm -f $(MODVERSIONS_H).tmp ; exit 1 ; } ; \
	mv -f $(MODVERSIONS_H).tmp $@

