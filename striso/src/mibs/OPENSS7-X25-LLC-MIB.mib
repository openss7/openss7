lLCConnectionlessTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF LLCConnectionlessEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"lLCConnectionless MOC. This clause contains those 
		 attributes, actions, and event notifications associated 
		 with an LLC connectionless environment.  This managed 
		 object class is responsible for processing the 
		 attributes, actions and notifications that are 
		 associated with the LLC connectionless service.
		 
		 The managed object class is derived from the lLCCLPM 
		 LLC connectionless protocol machine managed object 
		 class, and is also contained by an instance of the 
		 lLCCLPM with the lLCClessName as the relative name."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), lLCConnectionless MOC"
	::= { lLCConnectionless 1 }

lLCConnectionlessEntry OBJECT-TYPE
	SYNTAX		LLCConnectionlessEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		""
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2), lLCConnectionless MO"
	INDEX	{
			lLCCLPMProtocolMachineId, -- contained by lLCCLPM
			lLCCLPMProtocolMachineId -- derived from lLCCLPM
		}
	::= { lLCConnectionlessTable 1 }

LLCConnectionlessEntry ::=
	SEQUENCE {
		-- lLCConnectionless-P mandatory package
		lLCConnectionlessName
			DisplayString,
		lLCConnectionlessMaximumLLCInformationFieldSize
			INTEGER,
		lLCConnectionlessTESTReceivedABBResponse
			Counter32,
		lLCConnectionlessTESTReceivedCommand
			Counter32,
		lLCConnectionlessTESTReceivedResponse
			Counter32,
		lLCConnectionlessTESTSentABBResponse
			Counter32,
		lLCConnectionlessTESTSentCommand
			Counter32,
		lLCConnectionlessTESTSentResponse
			Counter32,
		lLCConnectionlessUIReceived
			Counter32,
		lLCConnectionlessUISent
			Counter32,
		lLCConnectionlessXIDReceivedCommand
			Counter32,
		lLCConnectionlessXIDReceivedResponse
			Counter32,
		lLCConnectionlessXIDSentCommand
			Counter32,
		lLCConnectionlessXIDSentResponse
			Counter32
		-- need action tESTSendCommand
		-- need action xIDSendCommand
	}

-- lLCConnectionless-P mandatory package
lLCConnectionlessName OBJECT-TYPE
	SYNTAX		DisplayString
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides the name of the higher level entity associated 
		 with this object class.
		 
		 Because the lLCConnectionlessName is used as a relative name 
		 within the containing lLCCLPM table entry, all 
		 lLCConnectionless entries with the same indexed lLCCLPM 
		 table entry must have unique names."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) lLCClessName."
	::= { lLCConnectionlessEntry 1 }

lLCConnectionlessMaximumLLCInformationFieldSize OBJECT-TYPE
	SYNTAX		INTEGER
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"The maximum length SDU that the LSAP will accomodate.  
		 For Ethernet 10baseT and 100baseT LANs this is 
		 typically the 1500 byte MTU minus the SNAP header, 
		 minus the address fields and command field, or 1492 
		 bytes."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) 
		 maximumLLCInformationFieldSize."
	::= { lLCConnectionlessEntry 2 }

lLCConnectionlessTESTReceivedABBResponse OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses received by this 
		 LSAP without the LSDU due to limited resources at the 
		 remote station."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) tESTReceivedABBResponse."
	::= { lLCConnectionlessEntry 3 }

lLCConnectionlessTESTReceivedCommand OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST commands received from the MAC 
		 sublayer by this LSAP.  This includes commands recieved 
		 with any group address recognized by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) tESTReceivedCommand."
	::= { lLCConnectionlessEntry 4 }

lLCConnectionlessTESTReceivedResponse OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses received from the 
		 MAC sublayer by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) tESTReceivedResponse."
	::= { lLCConnectionlessEntry 5 }

lLCConnectionlessTESTSentABBResponse OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses that were delivered 
		 to the MAC without the LSDU data due to limited 
		 resources."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) tESTSentABBResponse."
	::= { lLCConnectionlessEntry 6 }

lLCConnectionlessTESTSentCommand OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST command set to the MAC 
		 sublayer by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) tESTSentCommand."
	::= { lLCConnectionlessEntry 7 }

lLCConnectionlessTESTSentResponse OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of TEST responses sent to the MAC 
		 sublayer by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) tESTSentResponse."
	::= { lLCConnectionlessEntry 8 }

lLCConnectionlessUIReceived OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of user data PDUs that were received 
		 by the LSAP from the MAC sublayer and passed to the 
		 user layer.  This includes commands received with group 
		 addresses of which the LSAP is a member."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) uIReceived."
	::= { lLCConnectionlessEntry 9 }

lLCConnectionlessUISent OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of user data PDUs that were accepted 
		 by the LSAP from the user layer and delivered to the 
		 MAC sublayer."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) uISent."
	::= { lLCConnectionlessEntry 10 }

lLCConnectionlessXIDReceivedCommand OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Proivdes a count of XID commands received from the MAC 
		 sublayer by this LSAP.  This includes commands received 
		 with any group address recognized by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) xIDReceivedCommand."
	::= { lLCConnectionlessEntry 11 }

lLCConnectionlessXIDReceivedResponse OBJECT-TYPE
	SYNTAX		Counter32
	MAX-ACCESS	read-write
	STATUS		current
	DESCRIPTION
		"Provides a count of XID responses received from the MAC 
		 sublayer by this LSAP."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-2) xIDReceivedResponse."
