sLPConnectionTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF SLPConnectionEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The LAPB Single Link Protocol Connection managed 
		 object.  The sLPConnection managed object represents 
		 the local view of a connection between LAPB DLEs over a 
		 single physical link.
		 
		 An sLPConnection MO is created automatically as part of 
		 a system operation.
		 
		 An sLPConnection MO may be deleted automatically as a 
		 part of system operation or may be deleted as a result 
		 of the deactivate or delete management operations.
		 
		 An sLPConnectionIVMO may be used as the source of 
		 initial values of attributes of an sLPConnection MO.

		 This table provides the state of all LAPB connections.

		 This table provides the statistics associated with a 
		 given LAPB connection which is associated with a 
		 specific LAPB interface."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742, sLPConnection."
	::= { sLPConnection 1 }

sLPConnectionEntry OBJECT-TYPE
	SYNTAX		SLPConnectionEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"This is an entry in the state table representing a 
		 single LAPB data link connection.

		 This table entry provides detailed statistics for a 
		 given LAPB connection associatedw with a given LAPB 
		 interface."
	REFERENCE
		"ITU-T Rec. X.282 | ISO/IEC 10742."
	INDEX		{ }
	::= { sLPConnectionTable 1 }

SLPConnectionEntry ::=
	SEQUENCE {
		-- derived from singlePeerConnection
		sLPConnectionConnectionId
			DisplayString,
		sLPConnectionUnderlyingConnectionNames
			RowPointer,
		-- singlePeerConnection singlePeerConnectionP2 package
		sLPConnectionSupportedConnectionNames
			RowPointer,
		-- mandatory commonSLPnnection-P package
		sLPConnectionInterfaceType
			INTEGER,
		sLPConnectionK
			INTEGER,
		sLPConnectionN1
			INTEGER,
		sLPConnectionN2
			INTEGER,
		sLPConnectionSequenceModulus
			INTEGER,
