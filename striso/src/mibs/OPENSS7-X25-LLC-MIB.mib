-- ====================================================================
--
-- Notifications
--

lLCStationEvent NOTIFICATION-TYPE
	OBJECTS	{
			objectInstance
			-- pdusDiscarded
			-- bufferProblems
			-- ?? retransmissions
		}
	STATUS		current
	DESCRIPTION
		"This notification will be sent for either PDUs 
		 discarded, buffer problems, or type 3 retransmissions."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-3), lLCStationEvent-N."
	::= { x25Notifications 1 }

lLCConnection2Event NOTIFICATION-TYPE
	OBJECTS	{
			objectInstance
			-- pdusDiscarded1
			-- pdusDiscarded2
			-- pduRetransmissions
			-- acknowledgeTimeout
			-- busyStateTimeout
			-- rejectTimeout
			-- pBitTimeout
			-- type2Violation
			-- retransmissions
		}
	STATUS		current
	DESCRIPTION
		"This event will be sent for the following conditions: 
		 duplicate IPDUs received, PDUs discarded 1, PDUs 
		 discarded 2, PDU retransmissions, type2 violations, 
		 type2 acknowledgement timeout value, type 2 reject 
		 timeout value, type 2 busy state, timeout value type 2, 
		 p-bit timeout value, retransmissions.  For each 
		 condition, there is an appropriate parameter to be 
		 included in the notification."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-3), lLCConnection2Event-N."
	::= { x25Notifications 2 }

lLCClessACKEvent NOTIFICATION-TYPE
	OBJECTS	{
			objectInstance
			-- type3Violation
			-- retransmissions
			-- noResponse
		}
	STATUS		current
	DESCRIPTION
		"This event will be sent for the following conditions: 
		 retransmissions, no response, type 3 violation.  For 
		 each condition, there is an appropriate parameter to be 
		 included in the notification."
	REFERENCE
		"IEEE 802.2 (ISO/IEC 8802-3), lLCClessACKEvent-N."
	::= { x25Notifications 3 }


